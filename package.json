{
    "name": "flow-highlight",
    "displayName": "flow-highlight",
    "description": "Flow.hs language highlighting",
    "version": "1.2.0",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "flowhs",
            "aliases": ["flow", "flowhs"],
            "extensions": [".hf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "flowhs",
            "scopeName": "source.flowhs",
            "path": "./syntaxes/flowhs.tmLanguage.json"
        }]
    }
}